function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               fc3
    set -U fish_color_param                 white -o
    set -U fish_color_redirection           white -o
    set -U fish_color_comment               666
    set -U fish_color_error                 666
    set -U fish_color_escape                fc3
    set -U fish_color_operator              fc3
    set -U fish_color_end                   09f
    set -U fish_color_quote                 09f
    set -U fish_color_autosuggestion        555
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   fc3
    set -U fish_color_cwd_root              fc3
    set -U fish_color_match                 fc3
    set -U fish_color_search_match          --background=09f
    set -U fish_color_selection             --background=09f
    set -U fish_pager_color_prefix          white
    set -U fish_pager_color_completion      white -o
    set -U fish_pager_color_description     white
    set -U fish_pager_color_progress        fc3
    set -U fish_color_history_current       fc3
end
